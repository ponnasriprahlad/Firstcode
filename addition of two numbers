#include <stdio.h>
void calculateSum(int *a, int *b, int *result) {
    *result = *a + *b;
}
int main() {
    int num1, num2, sum;
    printf("Enter two integers: ");
    if (scanf("%d %d", &num1, &num2) != 2) {
        printf("Invalid input.\n");
        return 1;
    }
    calculateSum(&num1, &num2, &sum);
    printf("The sum of %d and %d is: %d\n", num1, num2, sum);
    return 0;
}
